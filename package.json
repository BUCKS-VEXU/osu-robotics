{
  "name": "osu-robotics",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,css,json,md}\" --write",
    "dev": "concurrently -k -n api,web -c blue,magenta \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "npm run prisma:generate && tsx watch src/server/server.ts",
    "dev:client": "vite --host",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build": "npm run prisma:generate && npm run build:client && npm run build:server",
    "start": "node dist/server/server.js",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.2",
    "@mui/x-charts": "^8.11.2",
    "@prisma/client": "^6.16.2",
    "connect-pg-simple": "^10.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "install": "^0.13.0",
    "node-fetch": "^3.3.2",
    "npm": "^11.6.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-lottie-player": "^2.1.0",
    "react-router-dom": "^7.9.3",
    "react-social-media-embed": "^2.5.18",
    "use-window-focus": "^1.4.3",
    "vite-tsconfig-paths": "^4.3.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.5.2",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.14",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^6.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "prisma": "^6.16.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  }
}